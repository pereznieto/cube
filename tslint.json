{
    "extends": [
        "tslint-config-airbnb",
        "tslint-react"
    ],
    "rules": {
        "max-line-length": false,
        "import-name": false,
        "object-shorthand-properties-first": false,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "variable-name": [
            true,
            "ban-keywords"
        ]
    }
}
