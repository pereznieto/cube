{
  "name": "cube",
  "version": "0.0.1",
  "private": false,
  "homepage": "https://github.com/pereznieto/cube#cube-timer",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "tslint -p ./tsconfig.json -c ./tslint.json 'src/**/*.+(js|ts|tsx)'",
    "serve": "serve -s build",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "html-react-parser": "^0.4.6",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.1.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "node-sass": "^4.8.3",
    "numeral": "^2.0.6",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.109",
    "@types/node": "^9.6.4",
    "@types/node-sass": "^3.10.32",
    "@types/numeral": "^0.0.22",
    "@types/react": "^16.3.8",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-mock-store": "^0.0.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "react-dev-utils": "^5.0.1",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "1.4.0",
    "resolve-url-loader": "^2.3.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-airbnb-base": "^0.1.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setupTests.js",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "tslint-config-airbnb",
      "tslint-react"
    ]
  }
}
